# Insydetradar Tactical Environment Template
# Copy this to .env and fill in the secrets

# Database
DATABASE_URL=postgresql://postgres:[PASSWORD]@aws-0-eu-west-2.pooler.supabase.com:6543/postgres

# Supabase Auth (Public)
EXPO_PUBLIC_SUPABASE_URL=https://[PROJECT_REF].supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=[ANON_KEY]

# Supabase Auth (Private)
SUPABASE_SERVICE_ROLE_KEY=[SERVICE_ROLE_KEY]

# Alpaca Broker (Global Autonomous Engine)
ALPACA_API_KEY=[YOUR_KEY]
ALPACA_SECRET_KEY=[YOUR_SECRET]
ALPACA_PAPER=true

# Stripe Payments
STRIPE_SECRET_KEY=[YOUR_STRIPE_KEY]
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=[YOUR_STRIPE_PUB_KEY]

# App Configuration
PORT=3000
COOKIE_SECRET=insydetradar_secret_protocol_777
OWNER_OPEN_ID=[YOUR_SUPABASE_USER_ID_FOR_ADMIN]
